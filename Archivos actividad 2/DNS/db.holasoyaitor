;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	holasoyaitor.tk. root.holasoyaitor.tk (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	dns.holasoyaitor.tk.


dns.holasoyaitor.tk.	IN	A	192.168.0.2

@	IN	MX	10	mail.holasoyaitor.tk.


mail	IN	A	192.168.0.4
web	IN	A	192.168.0.3
